{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"test": {
			"dependsOn": ["^build"],
			"inputs": ["src/**/*.ts", "src/**/*.tsx", "test/**/*.ts", "test/**/*.tsx"]
		},
		"lint": {
			"inputs": ["src/**/*.ts", "src/**/*.tsx"]
		},
		"dev": {
			"dependsOn": ["^build"],
			"cache": false
		},
		"clean": {
			"cache": false
		},
		"website#build": {
			"outputs": [".next/**", "!.next/cache/**", "out/**"]
		}
	}
}
