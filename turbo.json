{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"dev": {
			"dependsOn": ["^build"],
			"outputLogs": "new-only",
			"cache": false
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"],
			"outputLogs": "new-only"
		},
		"test": {
			"dependsOn": ["^build"],
			"outputLogs": "new-only"
		},
		"clean": {
			"cache": false
		},
		"website#build": {
			"outputs": [".next/**", "!.next/cache/**", "out/**"]
		}
	}
}
