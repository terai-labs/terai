{
	"name": "@terai/studio",
	"version": "0.0.13",
	"description": "Terai Studio - Visual translation management interface",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"private": false,
	"author": "Hugo Corta <corta.hugo@gmail.com>",
	"scripts": {
		"dev": "tsdown --format=esm --no-dts --watch",
		"build:server": "tsdown",
		"build:client": "vite build",
		"clean": "rimraf dist",
		"build": "pnpm clean && pnpm build:server && pnpm build:client"
	},
	"files": [
		"dist"
	],
	"sideEffects": false,
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"dependencies": {
		"@terai/runtime": "workspace:*",
		"@terai/types": "workspace:*",
		"@terai/logger": "workspace:*",
		"open": "^10"
	},
	"devDependencies": {
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@vitejs/plugin-react": "^4",
		"react": "^19",
		"react-dom": "^19",
		"react-router": "^7",
		"vite": "^6"
	}
}
