{
	"name": "@terai/utils",
	"version": "0.0.12",
	"description": "terai shared utils",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"author": "Hugo Corta <corta.hugo@gmail.com>",
	"scripts": {
		"dev": "tsdown --format=esm --no-dts --watch",
		"build": "tsdown"
	},
	"files": [
		"dist"
	],
	"sideEffects": false,
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"devDependencies": {
		"@terai/types": "workspace:*"
	}
}
